<UserControl x:Class="amDevFl.View.LaunchPad"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:amDevFl.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
		<TextBlock VerticalAlignment="Top" Foreground="{StaticResource AMForeground}">
			<Run>Tasks</Run>
			<LineBreak />	
			<Line Margin="7" X1="0" X2="200" />
		<LineBreak />
		<Hyperlink Tag="Code" Click="hyp_Click">Create new Code Document </Hyperlink>
			
	<LineBreak />
		<Hyperlink ToolTip="Create new Writer Document " Tag="Writer" Click="hyp_Click">Create new Writer Document </Hyperlink>
		
			<LineBreak />
		<Hyperlink ToolTip="Create new Browser Flow " Tag="Browser" Click="hyp_Click">Create new Browser Flow </Hyperlink>
	
		<LineBreak />
		<Hyperlink Tag="Msg" Click="hyp_Click">Create new Web Msg  </Hyperlink>

		</TextBlock>    
    </Grid>
</UserControl>
