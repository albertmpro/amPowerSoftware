<local:FlowPage x:Class="amFlowDoczBase.View.TabPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:c="clr-namespace:Albert.Standard.Win32;assembly=AlbertWPF"
      xmlns:local="clr-namespace:amFlowDoczBase.Controls"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="TabPage">

    <Grid>
        <TabControl Grid.Row="1" x:Name="tab">
            <c:DocumentTabItem Header="Start">
                <local:LaunchPad />
            </c:DocumentTabItem>
        </TabControl>
    </Grid>
</local:FlowPage>
